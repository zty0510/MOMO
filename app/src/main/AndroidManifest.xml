<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.momo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/actions" />

        <activity android:name=".View.HomeActivity" />
        <activity android:name=".View.WrongBook" />
        <activity android:name=".View.FinsihActivity" />
        <activity android:name=".View.WordActivity" />
        <activity
            android:name=".View.Dictionary_Choose"
            android:exported="true"
            android:theme="@style/QMUI.Compat.NoActionBar" />
        <activity
            android:name=".View.Wordlist_Choose"
            android:theme="@style/QMUI.Compat.NoActionBar" />
        <activity
            android:name=".View.HelloActivity"
            android:theme="@style/QMUI.Compat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>